g)select deptno, count(empno) from dbo.emp group by deptno

h) select deptno, count(sal) from dbo.emp group by deptno

i) select job , count(empno) from dbo.emp  order by job desc

j) select sum(sal), max(sal), min(sal), avg(sal) from  dbo.emp group by job

k) select sum(sal), max(sal), min(sal) , avg(sal) from dbo.emp where deptno=20

